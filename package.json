    "gatsby-transformer-plaintext": "^2.0.0",
    "gatsby-transformer-remark": "^6.0.0",
    "libsodium-wrappers": "^0.7.10",
    "node-sass": "^9.0.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
